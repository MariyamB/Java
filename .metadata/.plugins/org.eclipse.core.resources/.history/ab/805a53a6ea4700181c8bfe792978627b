package com.maria.rajeev;

public class JadenCase {
	public static void main(String[] args)
	{
		System.out.println("most trees are blue");
	}

	public String toJadenCase(String phrase) {
		String[] splited = phrase.split(" ");
		String str=" ";
		System.out.print(splited);
		for(int i=0;i< splited.length;i++)
		{
			str=splited[i].toString().substring(1,2).toUpperCase();
		}
		return str;
	}

}